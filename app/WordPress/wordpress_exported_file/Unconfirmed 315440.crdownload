		<description></description>
		<content:encoded><![CDATA[<!-- wp:columns -->
<div class="wp-block-columns"><!-- wp:column {"width":"100%"} -->
<div class="wp-block-column" style="flex-basis:100%"><!-- wp:html -->
<!-- Fixed Header -->
<div style="position: fixed;width: 100%;top: 0;left: 0;background: #010170;color: white;padding: 1.5rem 2rem;z-index: 9999;display: flex;justify-content: space-between;align-items: center;font-family: sans-serif">
  <!-- Logo -->
  <a href="/" style="display: flex;align-items: center">
    <img src="https://bravur.webplaatsproject.nl/wp-content/uploads/2025/04/Bravur.svg" alt="Bravur Logo" style="height: 40px">
  </a>

  <!-- Navigation Links -->
  
    <a href="/professionals/" style="color: white;text-decoration: none">Professionals</a>
    <a href="/over-ons/" style="color: white;text-decoration: none">Over ons</a>
    <a href="/opdrachtgevers/" style="color: white;text-decoration: none">Opdrachtgevers</a>
    <a href="/contact/" style="color: white;text-decoration: none">Contact</a>
  

  <!-- Like Icon and Counter -->
  <div style="position: relative;margin-left: 1rem">
    <img src="https://img.icons8.com/material-rounded/24/ffffff/like.png" />
    <span style="position: absolute;top: -8px;right: -10px;background: #00B2FF;padding: 0 6px;font-size: 0.75rem;border-radius: 9999px">2</span>
  </div>

  <!-- Vacature Button -->
  <a href="/vacatures/" style="background: #00B2FF;margin-left: 1rem;padding: 0.6rem 1rem;border-radius: 0.5rem;color: white;font-size: 1rem;text-decoration: none">
    Vacatures <span style="background: #010170;padding: 0 6px;margin-left: 0.5rem;border-radius: 9999px">46</span>
  </a>
</div>

<!-- Spacer for fixed header -->
<div style="height: 100px"></div>
<!-- /wp:html --></div>
<!-- /wp:column --></div>
<!-- /wp:columns -->

<section style="background-color: #020129;color: white;font-family: 'Lexend', sans-serif;padding: 4rem 1rem">
  <div style="max-width: 1200px;margin: auto;display: flex;flex-wrap: wrap;justify-content: space-between;align-items: center;gap: 2rem">
    
    <!-- Text Content -->
    <div style="flex: 1">
      <h1 style="font-size: 3rem;font-weight: 700;margin-bottom: 1rem">Be Boldy Human.</h1>
      <p style="font-size: 1.125rem;margin-bottom: 1.5rem;max-width: 600px;line-height: 1.6">
        Sta op. Kom in beweging.<br>
        Wij staan achter jou. Jij achter ons. Verbinding en waardering. Daar draait het écht om.
        Jij staat centraal, want zonder jou zijn we nergens. We zijn gelijkwaardig.
      </p>
      <div>
        <a href="/digitale-scan/" target="_blank" style="display: inline-block;padding: 0.75rem 1.5rem;font-weight: 600;font-size: 1rem;border-radius: 0.5rem;text-decoration: none;margin-right: 1rem;background-color: #fc03e8;color: white">
          Doe de digitale scan
        </a>
        <a href="/vacatures/" style="display: inline-block;padding: 0.75rem 1.5rem;font-weight: 600;font-size: 1rem;border-radius: 0.5rem;text-decoration: none;border: 2px solid #fc03e8;color: #fc03e8;background-color: transparent">
          Vacatures
        </a>
      </div>
    </div>

    <!-- Image -->
    <div style="flex: 1;text-align: center">
      <img src="https://bravur.webplaatsproject.nl/wp-content/uploads/2025/04/be_boldly_human-800x800.webp" alt="Be Boldly Human" style="max-width: 100%;border-radius: 1rem;height: auto">
    </div>
  </div>
