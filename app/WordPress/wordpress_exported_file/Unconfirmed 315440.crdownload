    </div>
  </div>
</section>

<!-- /wp:html -->

<!-- wp:html -->
<div class="fixed bottom-4 right-4 w-80 max-w-full bg-white text-black shadow-xl rounded-lg overflow-hidden z-50">
  <div class="bg-[#010170] text-white text-center py-2 font-semibold">Chatbot</div>
  <div class="p-4 h-64 overflow-y-auto">
    <p class="text-sm italic text-gray-600">Hallo! Waarmee kan ik je helpen?</p>
    <!-- Future: Append messages here -->
  </div>
  <div class="border-t flex items-center px-3 py-2">
    <input type="text" placeholder="Type here..." class="flex-1 border-none outline-none text-sm p-2" />
    <button class="ml-2 bg-[#010170] text-white px-3 py-1 rounded">Send</button>
  </div>
</div>

<!-- /wp:html -->]]></content:encoded>
